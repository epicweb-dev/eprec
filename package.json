{
  "name": "eprec",
  "type": "module",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/epicweb-dev/eprec"
  },
  "scripts": {
    "app:start": "bun --watch ./src/cli.ts app start",
    "format": "prettier --write .",
    "test": "bun test process-course src/utils.test.ts",
    "test:e2e": "bun test ./e2e",
    "test:smoke": "bunx playwright test -c playwright/playwright-smoke-config.ts",
    "test:all": "bun test '**/*.test.ts'",
    "validate": "bun run test"
  },
  "bin": {
    "eprec": "./src/cli.ts"
  },
  "files": [
    "app/**",
    "process-course/**",
    "public/**",
    "server/**",
    "src/**"
  ],
  "prettier": "@epic-web/config/prettier",
  "devDependencies": {
    "@epic-web/config": "^1.21.3",
    "@playwright/test": "^1.58.0",
    "@types/bun": "latest",
    "@types/yargs": "^17.0.35",
    "prettier": "^3.8.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "get-port": "^7.1.0",
    "inquirer": "^13.2.1",
    "onnxruntime-node": "^1.23.2",
    "ora": "^9.1.0",
    "remix": "3.0.0-alpha.0",
    "yargs": "^18.0.0"
  }
}
